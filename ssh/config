Include config.d/local

Host github.com
    HostName github.com
    ForwardAgent no
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host *
    User shivakar
    IdentityFile ~/.ssh/id_rsa
    IgnoreUnknown AddKeysToAgent,UseKeychain
    AddKeysToAgent yes
    UseKeychain yes
